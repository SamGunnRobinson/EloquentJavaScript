// Your code here.

var size = 8;
var board = ' ';

for (var horiz = 0; horiz < size; horiz ++)
	{
     for (var vert = 0; vert < size; vert ++) 
     	{
         if ((horiz + vert) % 2 === 0)
           board += ' ';
         else board += '#'; 
        }
      
 board += '\n'; 
    }
console.log(board);
     
